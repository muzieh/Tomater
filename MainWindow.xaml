<Window x:Class="Tomater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Tomater" Height="177" Width="294" Opacity="1" BorderBrush="#FF963636" BorderThickness="2"
		Style="{StaticResource {x:Type Window}}"
    WindowStartupLocation="CenterScreen" Topmost="True">
   
		<StackPanel Name="stackPanel3" Orientation="Vertical" Margin="12">
			<TextBlock  Name="textDateDisplay" Text="00:00" FontFamily="Courier New" FontSize="48" HorizontalAlignment="Center" />
			<StackPanel Name="stackPanel2"  Orientation="Horizontal"  HorizontalAlignment="Center">
			<Button Content="Work" Height="23" Name="buttonWork" Width="75" Click="buttonWork_Click"/>
			<Button Content="Short br" Height="23" Name="buttonShort" Width="75" Click="buttonShort_Click" />
			<Button Content="Long br" Height="23" Name="buttonLong" Width="75" Click="buttonLong_Click" />
		</StackPanel>
		<StackPanel ><Button Content="Void"  Width="75" Name="buttonVoid" Click="buttonVoid_Click" /></StackPanel>
		
	</StackPanel>
		
</Window>
